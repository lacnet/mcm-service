<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="Cleric" class="fr.lacnet.munchkin.mdm.card.door.classe.ClericBean">
		<constructor-arg><value>ClericBean</value></constructor-arg>
	</bean>

	<bean id="SuperMunchkin" class="fr.lacnet.munchkin.mdm.card.door.classe.SuperMunchkin">
		<constructor-arg><value>SuperMunchkin</value></constructor-arg>
	</bean>

	<bean id="Thief" class="fr.lacnet.munchkin.mdm.card.door.classe.ThiefBean">
		<constructor-arg><value>ThiefBean</value></constructor-arg>
	</bean>

	<bean id="Warrior" class="fr.lacnet.munchkin.mdm.card.door.classe.WarriorBean">
		<constructor-arg><value>WarriorBean</value></constructor-arg>
	</bean>

	<bean id="Wizard" class="fr.lacnet.munchkin.mdm.card.door.classe.WizardBean">
		<constructor-arg><value>WizardBean</value></constructor-arg>
	</bean>
</beans>
